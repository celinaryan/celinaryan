space-between;\n  align-items: center;\n  padding: 10px;\n}\n\n.logo {\n  display: flex; /* Make the logo and nav links display in a row */\n  align-items: normal; /* Vertically center logo with nav links */\n}\n\n.nav-links {\n  list-style: none;\n  display: flex; /* Display the nav links in a row */\n  align-items: center; /* Vertically center nav links with logo */\n}\n\n.nav-item {\n  align-items: right; /* Vertically align navigation items with logo */\n}\n\n/* Remove unnecessary align-items and align-items styles */\n.nav-item a {\n  display: inline-block;\n  padding: 10px 15px;\n  text-decoration: none;\n  color: white;\n}\n\n.nav-item:hover {\n  background-color: white;\n}\n\n.nav-item:hover a {\n  color: rgb(107, 164, 207);\n}\n\n/* Initially hide the mobile navigation menu */\n.mobile-nav-links {\n  display: none;\n  list-style: none;\n  padding: 0;\n}\n\n.logo img {\n  width: 80px;\n  padding-left: 5px;\n}\n/* trying to animate this guy*/\n.bar {\n  width: 30px;\n  height: 3px;\n  margin: 5px 0;\n  background-color: white;\n  display: block;\n  transition: all 0.3 ease;\n}\n/* Rotate the first bar */\n.toggle-button.change .bar:nth-child(1) {\n  transform: rotate(-45deg) translate(-5px, 6px);\n  transition: transform 0.3s ease-out;\n}\n\n/* Fade out the second bar */\n.toggle-button.change .bar:nth-child(2) {\n  opacity: 0;\n}\n\n/* Rotate the third bar */\n.toggle-button.change .bar:nth-child(3) {\n  transform: rotate(45deg) translate(-5px, -6px);\n  transition: transform 0.3s ease-out;\n}\n.navbar {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  width: 100%;\n  padding: 10px;\n  background-color: rgb(158, 202, 236);\n  position: relative;\n  z-index: 9999;\n}\n\n/* hide navigation menu on small screens */\n.nav-links {\n  display: flex;\n}\n\n.toggle-button {\n  display: none;\n  flex-direction: column;\n  justify-content: space-around;\n  height: 21px;\n}\n\n/* Add media query to show/hide elements based on screen width */\n@media (max-width: 768px) {\n  .toggle-button {\n    display: inline-block;\n    right: 0;\n  }\n\n  .nav-links {\n    display: none;\n  }\n\n  /* Display the mobile navigation menu when the hamburger icon is clicked */\n  .nav-links.active {\n    display: flex;\n    flex-direction: column;\n    background-color: rgb(158, 202, 236);\n    position: absolute;\n    right: 0;\n    top: 60px; /* Adjust this value as needed */\n    width: 100%;\n    justify-content: left;\n    align-items: center;\n  }\n\n  /* Style the mobile navigation links */\n  .nav-links.active li {\n    text-align: center;\n    align-items: stretch;\n    flex-direction: column;\n    display: flex;\n  }\n}\n\n\n@media (max-width: 768px) {\n  .toggle-button {\n    /* Keep at your needed display property when screen size is reduced */\n    display: flex;\n  }\n\n  .nav-links {\n    /* Display the nav-menu by default when hamburger icon is clicked */\n    display: none;\n    position: absolute;\n    right: 0;\n    height: 100vh; /* So that the background color covers the full height */\n    top: 60px;\n    flex-direction: column;\n    align-items: center;\n    justify-content: center;\n    background-color: rgb(158, 202, 236);\n    width: 100%;\n  }\n\n  .nav-links.active {\n     /* Change display to flex when hamburger icon is clicked (handled in your JS code) */\n    display: flex;\n  }\n}"},"lineCount":222}},"error":null,"hash":"862008484d0bdc53be911e2f54fa2927","cacheData":{"env":{}}}